<h1>All users</h1>

<%= will_paginate %>

<div class="search">
  <p>user search</p>
  <%= form_tag(users_path, :method => 'get' ) do %>
    <%= text_field_tag :search %>
    <%= submit_tag 'Search', :name => nil %>
  <% end %>
</div>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
